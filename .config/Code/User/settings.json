{
  "editor.fontFamily": "HackGen35",
  "editor.fontSize": 14,
  "editor.tabSize": 4,
  "editor.insertSpaces": false,
  "editor.renderControlCharacters": true,
  "editor.renderLineHighlight": "all",
  "editor.renderWhitespace": "all",
  "editor.occurrencesHighlight": false,
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": true,
  "editor.showFoldingControls": "always",
  "editor.accessibilitySupport": "off",
  "editor.formatOnType": true,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "modifications",
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "editor.tabCompletion": "on",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.hideCursorInOverviewRuler": true,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.showSlider": "always",
  "editor.minimap.scale": 2,
  "diffEditor.ignoreTrimWhitespace": false,
  "files.autoGuessEncoding": true,
  "files.encoding": "utf8",
  "files.eol": "\n",
  "files.defaultLanguage": "markdown",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "files.exclude": {
    "**/.vscode": true,
    "**/.venv": true,
    "**/.Rproj.user": true
  },
  "breadcrumbs.enabled": true,
  "workbench.startupEditor": "none",
  "workbench.colorTheme": "Panda Syntax",
  "workbench.colorCustomizations": {
    "[One Dark Pro]": {
      "editorWhitespace.foreground": "#a9a9a960",
      "editor.lineHighlightBackground": "#70707020",
      "editor.selectionBackground": "#5472d3",
      "editor.selectionHighlightBackground": "#0d47a180",
      "editor.selectionHighlightBorder": "#5472d3",
      "editor.findMatchBackground": "#ffd95a",
      "editor.findMatchBorder": "#ffd95a",
      "editor.findMatchHighlightBackground": "#f9a82580",
      "editor.findMatchHighlightBorder": "#ffd95a",
      "editor.findRangeHighlightBackground": "#5472d320",
      "minimapSlider.background": "#70707040",
      "minimapSlider.hoverBackground": "#70707080",
      "minimapSlider.activeBackground": "#70707080"
    },
    "[Night Owl]": {
      "activityBar.background": "#000C1D",
      "activityBar.border": "#102a44",
      "editorGroup.border": "#102a44",
      "sideBar.background": "#001122",
      "sideBar.border": "#102a44",
      "sideBar.foreground": "#8BADC1",
      "editorWhitespace.foreground": "#d6deeb60",
      "editor.lineHighlightBackground": "#1d3b5380"
    }
  },
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "comment",
          "comment.block",
          "comment.block.documentation",
          "comment.line",
          "punctuation.definition.comment",
          "keyword",
          "keyword.control",
          "keyword.control.flow.python",
          "keyword.control.import.python",
          "keyword.operator",
          "keyowrd.other",
          "variable",
          "variable.language",
          "variable.parameter",
          "variable.other"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },
  "workbench.iconTheme": "material-icon-theme",
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "window.restoreWindows": "none",
  "window.title": "${dirty}${activeEditorMedium}${separator}${rootName}",
  "window.zoomLevel": 1,
  "terminal.integrated.defaultProfile.windows": "PowerShell Core",
  "terminal.integrated.profiles.windows": {
    "PowerShell Core": {
      "path": "${env:USERPROFILE}\\scoop\\apps\\pwsh\\current\\pwsh.exe",
      "args": [
        "-NoLogo"
      ],
      "overrideName": true,
      "icon": "terminal-powershell"
    },
    "PowerShell": {
      "source": "PowerShell",
      "icon": "terminal-powershell"
    },
    "Command Prompt": {
      "path": [
        "${env:windir}\\Sysnative\\cmd.exe",
        "${env:windir}\\System32\\cmd.exe"
      ],
      "args": [],
      "icon": "terminal-cmd"
    },
    "Git Bash": {
      "source": "Git Bash"
    }
  },
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.fontFamily": "HackGenNerd Console",
  "terminal.integrated.fontSize": 14,
  "telemetry.telemetryLevel": "off",
  "[markdown]": {
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint",
    "files.trimTrailingWhitespace": false
  },
  "[python]": {
    "editor.rulers": [
      79,
      119
    ],
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnPaste": false,
    "editor.formatOnSaveMode": "file",
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "black-formatter.args": [
    "--line-length=119"
  ],
  "isort.args": [
    "--profile",
    "black",
    "--line-length",
    "119"
  ],
  "code-eol.newlineCharacter": "↓",
  "code-eol.returnCharacter": "←",
  "code-eol.crlfCharacter": "↵",
  "git.autofetch": true,
  "gitlens.defaultDateFormat": "YYYY-MM-DD hh:mm",
  "gitlens.defaultDateShortFormat": "YYYY-MM-DD",
  "markdown.preview.fontFamily": "HackGen",
  "markdown.preview.fontSize": 14,
  "markdownlint.config": {
    "commands-show-output": false
  },
  "markdown-pdf.styles": [
    "style/markdown-pdf.css"
  ],
  "markdown-pdf.displayHeaderFooter": false,
  "material-icon-theme.saturation": 0,
  "oneDarkPro.markdownStyle": false,
  "powershell.powerShellDefaultVersion": "PowerShell Core",
  "powershell.powerShellAdditionalExePaths": {
    "PowerShell Core": "${env:USERPROFILE}\\scoop\\apps\\pwsh\\current\\pwsh.exe"
  },
  "powershell.integratedConsole.showOnStartup": false,
  "python.languageServer": "Pylance",
  "python.analysis.typeCheckingMode": "basic",
  "python.analysis.completeFunctionParens": true,
  "python.autoComplete.addBrackets": true,
  "python.linting.pylintEnabled": false,
  "python.linting.flake8Enabled": true,
  "python.linting.flake8Args": [
    "--max-line-length=119",
    "--extend-ignore=E203"
  ],
  "python.formatting.provider": "none",
  "python.formatting.blackArgs": [
    "--line-length=119"
  ],
  "python.formatting.autopep8Args": [
    "--max-line-length=119",
    "--aggressive",
    "--aggressive"
  ],
  "python.sortImports.args": [
    "--profile=black",
    "--line-length=119"
  ],
  "r.rterm.windows": "${env:APPDATA}\\Python\\Python310\\Scripts\\radian.exe",
  "r.rterm.option": [
    "--quiet",
    "--no-save",
    "--no-restore"
  ],
  "r.bracketedPaste": true,
  "r.sessionWatcher": true,
  "r.lsp.debug": true,
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
}
